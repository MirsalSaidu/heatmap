{
  "version": 2,
  "builds": [
    { "src": "server/index.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "src/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server/index.js" },
    { "src": "/dashboard", "dest": "dashboard.html" },
    { "src": "/", "dest": "dashboard.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
